{
  "name": "luca-autonomous-agent",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "platforms/electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node server.js",
    "cleanup": "./ops/scripts/cleanup.sh",
    "restore": "node ops/scripts/restore.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "safety-check": "npm run lint && npm run type-check && node ops/scripts/verify_integrity.cjs",
    "mobile:init": "npm run build && node ops/scripts/build-mobile.js init",
    "mobile:sync": "npm run build && node ops/scripts/build-mobile.js sync",
    "mobile:android": "npm run mobile:sync && node ops/scripts/build-mobile.js open android",
    "mobile:ios": "npm run mobile:sync && node ops/scripts/build-mobile.js open ios",
    "mobile:build:android": "npm run mobile:sync && cd android && ./gradlew assembleRelease",
    "mobile:build:ios": "npm run mobile:sync && cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release archive",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on tcp:3000 && electron .\"",
    "rebuild": "electron-rebuild",
    "postinstall": "npm rebuild better-sqlite3",
    "phoenix": "node ops/phoenix/supervisor.js",
    "migrate-skills": "node scripts/migrate-skills.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "*",
    "@google/generative-ai": "^0.24.1",
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "@monaco-editor/react": "^4.6.0",
    "@octokit/rest": "^22.0.1",
    "@picovoice/porcupine-web": "^3.0.3",
    "@picovoice/web-voice-processor": "^4.0.9",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@tensorflow-models/speech-commands": "^0.5.4",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-data": "^4.22.0",
    "@tensorflow/tfjs-layers": "^4.22.0",
    "active-win": "^8.2.1",
    "better-sqlite3": "^12.5.0",
    "ccxt": "^4.5.27",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^5.0.0",
    "chromium-bidi": "^12.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "ethers": "^6.16.0",
    "eventsource": "^4.1.0",
    "express": "^4.18.2",
    "framer-motion": "^12.23.26",
    "google-auth-library": "^10.5.0",
    "googleapis": "^168.0.0",
    "https-proxy-agent": "^7.0.2",
    "hyperliquid": "^1.7.7",
    "idb": "^8.0.3",
    "input": "^1.0.1",
    "js-yaml": "^4.1.1",
    "lighter-node-client": "^1.0.0",
    "lightweight-charts": "^5.0.9",
    "localtunnel": "^2.0.2",
    "lucide-react": "^0.344.0",
    "mqtt": "^5.14.1",
    "msgpack-lite": "^0.1.26",
    "multer": "^2.0.2",
    "openai": "^6.10.0",
    "playwright": "^1.57.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-3d": "^1.29.0",
    "react-json-view": "^1.21.3",
    "recharts": "^2.12.2",
    "robotjs": "^0.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "socks-proxy-agent": "^8.0.2",
    "technicalindicators": "^3.1.0",
    "telegram": "^2.26.22",
    "three": "^0.158.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "wake_on_lan": "^1.0.0",
    "whatsapp-web.js": "^1.23.0",
    "ws": "^8.16.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
    "@capacitor/ios": "^6.2.1",
    "@electron/rebuild": "^4.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/crypto-js": "^4.2.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.luca.ai",
    "productName": "Luca OS",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "dist/**/*",
      "platforms/electron/**/*",
      "server.js",
      "package.json",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
