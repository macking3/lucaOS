# LUCA OS Environment Configuration
# Copy this file to .env and fill in your API keys
# DO NOT commit .env to git - it contains sensitive credentials

# Google Gemini API Key (Required)
# Get your key from: https://makersuite.google.com/app/apikey
VITE_API_KEY=your_gemini_api_key_here
API_KEY=your_gemini_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Other LLM Provider Keys (if you want to use them)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# OLLAMA_BASE_URL=http://localhost:11434

# Google OAuth (for Gmail/Drive/Calendar integration)
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Backend API URL (for frontend to connect to local server)
# Change this if your backend runs on a different port or domain
# Default: http://localhost:3002
VITE_API_URL=http://localhost:3002
VITE_WS_PORT=3003
VITE_CORTEX_URL=http://localhost:8000
VITE_AUTH_DOMAIN=http://localhost:3001
VITE_FRONTEND_PORT=3000
AUTH_PORT=3001
VITE_RELAY_SERVER_URL=https://your-relay-app-name.vercel.app
